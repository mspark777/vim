{
  "codeLens.enable": true,
  "eslint.autoFixOnSave": true,
  "tsserver.enableJavascript": true,
  "typescript.referencesCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "typescript.implementationsCodeLens.enable": false,
  "javascript.implementationsCodeLens.enable": false,
  "rust-analyzer.lens.implementations.enable": false,
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "~/.asdf/shims/black",
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.formatOnSaveFiletypes": ["python", "hcl"],
  "coc.preferences.formatOnType": true,
  "coc.preferences.willSaveHandlerTimeout": 3000,
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["hcl", "terraform", "tf"],
      "initializationOptions": {}
    }
  }
}
