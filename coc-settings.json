{
  "codeLens.enable": true,
  "eslint.autoFixOnSave": true,
  "typescript.referencesCodeLens.enabled": false,
  "javascript.referencesCodeLens.enabled": false,
  "javascript.implementationsCodeLens.enabled": false,
  "typescript.implementationsCodeLens.enabled": false,
  "rust-analyzer.lens.implementations.enable": false,
  "solargraph.formatting": true,
  "solargraph.autoformat": true,
  "solargraph.useBundler": true,
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "~/.asdf/shims/black",
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.formatOnSave": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.willSaveHandlerTimeout": 5000,
  "coc.preferences.extensionUpdateCheck": "daily",
  "languageserver": {
    "elixirLS": {
      "command": "~/.config/nvim/elixir-ls/language_server.sh",
      "filetypes": [
        "elixir",
        "eelixir"
      ]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {}
    }
  },
  "Lua.telemetry.enable": true
}
